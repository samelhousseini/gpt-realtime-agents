# Copy this file to .env and replace placeholder values with your Azure OpenAI details.

# Azure OpenAI Realtime Configuration
WEBRTC_URL="https://<region>.realtimeapi-preview.ai.azure.com/v1/realtimertc"
AZURE_GPT_REALTIME_URL="https://<resource-name>.openai.azure.com/openai/realtimeapi/sessions?api-version=2025-04-01-preview"
AZURE_GPT_REALTIME_KEY="<your-azure-openai-key>"
AZURE_OPENAI_ENDPOINT_WS="wss://<resource-name>.openai.azure.com"
AZURE_OPENAI_API_KEY="<your-azure-openai-key>"
AZURE_OPENAI_MODEL_NAME="gpt-realtime"

# Azure Communication Services Configuration
AZURE_ACS_CONN_KEY="endpoint=https://<your-acs-resource>.communication.azure.com/;accesskey=<your-access-key>"
ACS_PHONE_NUMBER="+1234567890"

# Callback Configuration
CALLBACK_EVENTS_URI="https://<your-callback-url>/api/callbacks"
CALLBACK_URI_HOST="wss:/<your-callback-url>"

# Azure Voice Configuration
AZURE_VOICELIVE_ENDPOINT="https://<endpoint>.cognitiveservices.azure.com/"
AZURE_VOICELIVE_API_KEY=""
AZURE_VOICELIVE_MODEL="gpt-realtime"
AZURE_VOICELIVE_VOICE="en-US-Ava:DragonHDLatestNeural"
AZURE_VOICELIVE_REGION="swedencentral"
AZURE_VOICELIVE_API_VERSION="2025-05-01-preview"
USE_VOICELIVE_FOR_ACS=true