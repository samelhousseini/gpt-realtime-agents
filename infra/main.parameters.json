{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "resourceGroupName": {
      "value": "${AZURE_RESOURCE_GROUP}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "webRTCUrl": {
      "value": "${WEBRTC_URL=https://<region>.realtimeapi-preview.ai.azure.com/v1/realtimertc}"
    },
    "gptRealtimeUrl": {
      "value": "${AZURE_GPT_REALTIME_URL=https://<resource-name>.openai.azure.com/openai/realtimeapi/sessions?api-version=2025-04-01-preview}"
    },
    "gptRealtimeKey": {
      "value": "${AZURE_GPT_REALTIME_KEY=<your-azure-openai-key>}"
    },
    "azureOpenAiEndpointWs": {
      "value": "${AZURE_OPENAI_ENDPOINT_WS=wss://<resource-name>.openai.azure.com}"
    },
    "azureOpenAiApiKey": {
      "value": "${AZURE_OPENAI_API_KEY=<your-azure-openai-key>}"
    },
    "azureOpenAiModelName": {
      "value": "${AZURE_OPENAI_MODEL_NAME=gpt-realtime}"
    },
    "azureAcsConnKey": {
      "value": "${AZURE_ACS_CONN_KEY=endpoint=https://<your-acs-resource>.communication.azure.com/;accesskey=<your-access-key>}"
    },
    "acsPhoneNumber": {
      "value": "${ACS_PHONE_NUMBER=+1234567890}"
    },
    "azureVoiceLiveEndpoint": {
      "value": "${AZURE_VOICELIVE_ENDPOINT=https://<endpoint>.cognitiveservices.azure.com/}"
    },
    "azureVoiceLiveApiKey": {
      "value": "${AZURE_VOICELIVE_API_KEY=<your-voice-live-api-key>}"
    },
    "azureVoiceLiveModel": {
      "value": "${AZURE_VOICELIVE_MODEL=gpt-realtime}"
    },
    "azureVoiceLiveVoice": {
      "value": "${AZURE_VOICELIVE_VOICE=en-US-Andrew:DragonHDLatestNeural}"
    },
    "azureVoiceLiveRegion": {
      "value": "${AZURE_VOICELIVE_REGION=swedencentral}"
    },
    "azureVoiceLiveApiVersion": {
      "value": "${AZURE_VOICELIVE_API_VERSION=2025-05-01-preview}"
    },
    "useVoiceLiveForAcs": {
      "value": "${USE_VOICELIVE_FOR_ACS=true}"
    },
    "audioBackendAppExists": {
      "value": false
    }
  }

}
